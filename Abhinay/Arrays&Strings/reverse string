class Solution {
public:
    void reverseString(vector<char>& s) {
        
        char temp;
  int n = s.size();
    // Move from begin and end. Keep
    // swapping strings. 
    int j = n - 1;
    for (int i = 0; i < j; i++) {
        temp = s[i];
        s[i] = s[j];
        s[j] = temp;
        j--;
    }
        
    }
};
